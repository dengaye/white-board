{
  "name": "white-board",
  "version": "0.4",
  "main": "index.js",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "scripts": {
    "start": "package-scripts start",
    "build": "package-scripts build",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,scss,md}\"",
    "lint": "lint-staged",
    "lint-fix": "eslint -c .eslintrc.json --cache --cache-location=.cache/.eslint --fix './src/**/*.{js,jsx,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{md,css,json}": [
      "prettier --write",
      "git add"
    ],
    "packages/(src/**/*.scss": [
      "prettier --write",
      "stylelint --config stylelint.config.js --syntax scss --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "immer": "^7.0.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recoil": "0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.6.3",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "packaging-scripts": "0.1.8",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "react-hot-loader": "^4.12.15",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^3.9.7"
  }
}
